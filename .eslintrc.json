{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier", "@typescript-eslint"],
  "extends": ["prettier"],
  "rules": {
    "no-debugger": "off",
    "no-dupe-args": "error",
    "no-dupe-keys": "off",
    "no-duplicate-case": "error",
    "no-ex-assign": "error",
    "no-extra-boolean-cast": "off",
    "no-extra-parens": "off",
    "no-extra-semi": "error",
    "no-func-assign": "error",
    "no-inner-declarations": "off",
    "no-invalid-regexp": "error",
    "no-negated-in-lhs": "error",
    "no-obj-calls": "error",
    "no-reserved-keys": "off",
    "no-sparse-arrays": "error",
    "use-isnan": "warn",
    "valid-jsdoc": "off",
    "valid-typeof": "error",
    "no-with": "warn",
    "radix": "off",
    "vars-on-top": "off",
    "wrap-iife": "warn",
    "yoda": ["warn", "never"],
    "strict": "off",
    "no-catch-shadow": "off",
    "no-delete-var": "warn",
    "no-label-var": "warn",
    "no-shadow-restricted-names": "error",
    "no-undef-init": "warn",
    "no-undef": "warn",
    "no-undefined": "warn"
  }
}
